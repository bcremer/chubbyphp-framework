{
    "name": "chubbyphp/chubbyphp-framework",
    "description": "A minimal Framework using PSR 3, PSR 7, PSR 11, PSR 15 and PSR 17.",
    "keywords": ["chubbyphp", "framework", "middleware"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Zogg",
            "email": "dominik.zogg@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "psr/container": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0.1",
        "psr/http-server-middleware": "^1.0.1",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "chubbyphp/chubbyphp-mock": "^1.4",
        "nikic/fast-route": "^1.3",
        "phpunit/phpunit": "^8.1",
        "phploc/phploc": "^5.0",
        "zendframework/zend-diactoros": "^2.1"
    },
    "autoload": {
        "psr-4": { "Chubbyphp\\Framework\\": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
