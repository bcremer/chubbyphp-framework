{
    "name": "chubbyphp/chubbyphp-framework",
    "description": "A minimal Framework using PSR 3, PSR 7, PSR 11, PSR 15 and PSR 17.",
    "keywords": [
        "chubbyphp",
        "framework",
        "microframework",
        "middleware",
        "requesthandler",
        "psr-3",
        "psr-7",
        "psr-11",
        "psr-15",
        "psr-17"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Zogg",
            "email": "dominik.zogg@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "psr/container": "^1.0",
        "psr/http-factory": "^1.0.1",
        "psr/http-message-implementation": "^1.0",
        "psr/http-message": "^1.0.1",
        "psr/http-server-handler": "^1.0.1",
        "psr/http-server-middleware": "^1.0.1",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "aura/router": "^3.1",
        "chubbyphp/chubbyphp-mock": "^1.4",
        "friendsofphp/php-cs-fixer": "^2.15.1",
        "guzzlehttp/psr7": "^1.4.2",
        "http-interop/http-factory-guzzle": "^1.0",
        "infection/infection": "^0.13.4",
        "nikic/fast-route": "^1.3",
        "nunomaduro/phpinsights": "^1.7",
        "nyholm/psr7": "^1.0",
        "phploc/phploc": "^5.0",
        "phpstan/phpstan": "^0.11.12",
        "phpunit/phpunit": "^8.3.3",
        "slim/psr7": "^0.5",
        "zendframework/zend-diactoros": "^2.0"
    },
    "autoload": {
        "psr-4": { "Chubbyphp\\Framework\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Chubbyphp\\Tests\\Framework\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
